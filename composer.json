{
    "name": "isgulkov/ethrpc",
    "description": "A thin PHP interface to Ethereum JSON RPC",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Ethereum",
        "ETH",
        "RPC",
        "cryptocurrency",
        "Laravel"
    ],
    "authors": [
        {
            "name": "Ilya Gulkov",
            "email": "x@isgulkov.ru"
        },
        {
            "name": "lee",
            "email": "jcsoft@aliyun.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "~5.3|~6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "IsGulkov\\EthRPC\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IsGulkov\\EthRPC\\EthRPCServiceProvider"
            ],
            "aliases": {
                "EthRPC": "IsGulkov\\EthRPC\\Facade\\EthRPC"
            }
        }
    }
}
